{
    "editor.wordWrap": "on",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "latexmk-unsafe",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-bibtex",
                "-pdflatex=pdflatex -synctex=1 -interaction=nonstopmode -shell-escape -file-line-error -output-directory='%OUTDIR%' '\\def\\UnsafeChapter{} \\input{%DOC%}'",
                "%DOC%"
            ]
        },
        {
            "name": "latexmk-casts",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-bibtex",
                "-pdflatex=pdflatex -synctex=1 -interaction=nonstopmode -shell-escape -file-line-error -output-directory='%OUTDIR%' '\\def\\CastsChapter{} \\input{%DOC%}'",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "latexmk Unsafe Chapter ðŸ”ƒ",
            "tools": [
                "latexmk-unsafe"
            ]
        },
        {
            "name": "latexmk Casts Chapter ðŸ”ƒ",
            "tools": [
                "latexmk-casts"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "lastUsed",
    "cSpell.allowCompoundWords": true,
    "cSpell.words": [
        "Intrinsics",
        "aload",
        "astore",
        "captionpos",
        "emphstyle",
        "instanceof",
        "upcast",
        "upcasts",
        "xleftmargin"
    ]
}